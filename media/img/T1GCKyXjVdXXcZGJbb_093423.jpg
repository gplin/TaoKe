        //EVENT = S.Event,
        Cookie = S.Cookie,
        VARNAME = 'p4presult',
        API_PREFIX = 'http://tns.simba.taobao.com/?name=tcmad&o=j&count=5&sw=1&pid=420780_1007&p4p=' + VARNAME;

    function HotItem(elm) {
        var self = this;
        // p4p容器
        self.container = S.get(elm);
        self._init();
    }

    S.augment(HotItem, {

        /**
         * 初始化方法
         * @protected
         */
        _init: function() {
            var self = this,
                getScript = S.getScript || S.Ajax.getScript;
            if (self.container) {
                getScript && getScript(self._buildAPI(), function() {
                    try {
                        self._buildDom();
                    } catch(e) {}
                });
            }
        },

        /**
         * 生成API
         */
        _buildAPI: function() {
            var u = API_PREFIX,
                self = this;

            u += '&url=' + self._getUrl()
                + '&ac=' + self._getAc()
                + '&ip=' + self._getIP()
                + '&rcn=' + self._getRcn()
                + '&wbt=' + encodeURIComponent(document.title);
            return u + '&t=' + S.now();
        },
        
        /**
         * 获取页面地址
         */
        _getUrl: function() {
            var url = "";
            if (location != parent.location) {
                url = document.referrer;
            } else {
                url = location.href;
            }
            return encodeURIComponent(url.substr(url.indexOf("://") + 3));
        },

        /**
         * 获取浏览用户acookie
         */
        _getAc: function() {
            var cna = Cookie.get('cna');
            if (!cna) return '';
            return encodeURIComponent(cna);
        },

        /**
         * 获取浏览用户IP地址
         */
        _getIP: function() {
            return DOM.attr(this.container, 'data-ip');
        },

        /**
         * 获取用户短期行为数据
         */
        _getRcn: function() {
            var wd = Cookie.get('wd');
            if (!wd) return '';
            wd = wd.replace(/_0db[\d]/g, '');
            var bids = (wd === "" ? [] : wd.split(',').slice(0, 3)), data = [];

            for (var i = 0; i < bids.length; ++i) {
                data.push(["0", "0", bids[i], "1"].join("\x03"));
            }

            return encodeURIComponent(data.join("\x02"));
        },

        /**
         * 生成DOM
         */
        _buildDom: function() {
            var jsonData = window[VARNAME],
                i, j, data, s,
                htmlArr = [];

            if (!jsonData || jsonData.length === 0) {
                return;
            }

            for (i = 0, j = 0; i < jsonData.length; i++) {
                data = jsonData[i];
                data['price'] = parseFloat(data['GOODSPRICE'] / 100).toFixed(2);
                data['TBGOODSLINK'] = data['TBGOODSLINK'].replace(/_sum\./, '_100x100.');
                s = '<li>' +
                    '<div class="pic"><a target="_blank" href="' + data['EURL'] + '" title="' + data['TITLE'] + '"><img src="' + data['TBGOODSLINK'] + '" /></a></div>' +
                    '<div class="price"><strong>' + data['price'] + '</strong></div>' +
                    '<div class="desc"><a target="_blank" href="' + data['EURL'] + '" title="' + data['TITLE'] + '">' + data['TITLE'] + '</a></div>' +
                    '</li>';
                htmlArr[j++] = s;
            }
            DOM.html(this.container, htmlArr.join(''));
        }
    });

    window.error_P4P = {
        HotItem: HotItem
    };

})();

</script>
<script>
    KISSY.ready(function() {
       new error_P4P.HotItem('#J_IndexP4P');
    });
</script>

